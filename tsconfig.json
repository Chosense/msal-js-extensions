{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "strict": false,
    "allowJs": true,
    "lib": ["es6", "dom"],
    "types": ["es6-promise"]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
